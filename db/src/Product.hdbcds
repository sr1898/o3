context Product {

	@Comment: 'Products entity'
    Entity Products{
    @Comment: 'Product ID'
     key PRODUCTID: String(10);
     @Comment: 'Product Category'
     CATEGORY: String(40);
     @Comment: 'Product Price'
     PRICE: Decimal(15,2);
    }; 

    Entity ProductLog{
     key PRODUCTID: String(10);
     key LOGID: Integer;
     key DATETIME: UTCDateTime;
     key USERNAME: String(80);
     LOGTEXT: String(500);
    };
};